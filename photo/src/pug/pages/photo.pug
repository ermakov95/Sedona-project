extends ../layout/main

block variables
  - var title = 'Седона';
  - var description = 'Седона — небольшой городок в Аризоне, заслуживающий большего!';
  - var pageClass = 'photo-page';

block content
  section.intro
    img.intro_text(src="assets/img/text-sedona.svg", alt="Welcome to Sedona")
    img.intro_triangle(src="assets/img/bg-intro-triangle-desktop.svg", alt="Triangle")
  section.title.heading-text
    h1.title-title.heading Фото и видео
    .title-text.text Не можете решиться на путешествие из-за курса? Фотографии помогут вам забыть о политике и экономике.
  section.photo
    .photo__wrapper 
      .photo__item
        picture.photo__item-picture
          source(media="(min-width: 1200px)", srcset="assets/img/photo-sunset-desktop@1x.webp, assets/img/photo-sunset-desktop@2x.webp 2x", type="image/webp", alt="Неродные просторы", loading="lazy")
          source(media="(min-width: 1200px)", srcset="assets/img/photo-sunset-desktop@1x.jpg, assets/img/photo-sunset-desktop@2x.jpg 2x", type="image/jpeg", alt="Неродные просторы", loading="lazy")
          source(media="(min-width: 768px)", srcset="assets/img/photo-sunset-tablet@1x.webp, assets/img/photo-sunset-tablet@2x.webp 2x", type="image/webp", alt="Неродные просторы", loading="lazy")
          source(media="(min-width: 768px)", srcset="assets/img/photo-sunset-tablet@1x.jpg, assets/img/photo-sunset-tablet@2x.jpg 2x", type="image/jpeg", alt="Неродные просторы", loading="lazy")
