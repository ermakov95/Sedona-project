extends ../layouts/main

block variables
  - var title = 'Седона';
  - var description = 'Седона — небольшой городок в Аризоне, заслуживающий большего!';
  - var nav__photo = true

block content
  section.intro
    img.intro__text(src="~@/img/text-sedona.svg", alt="Welcome to Sedona")
    img.intro__triangle(src="~@/img/bg-intro-triangle-desktop.svg", alt="Triangle")
  section.title.wrap
    h1.title__heading.wrap__heading Фото и видео
    .title__text.wrap__text Не можете решиться на путешествие из-за курса? Фотографии помогут вам забыть о политике и экономике.
  section.photo
    .inner
      .item
        picture.item__picture
          source(media="(min-width: 1200px)", srcset="~@/img/photo-sunset-desktop@1x.webp, ~@/img/photo-sunset-desktop@2x.webp 2x", type="image/webp", alt="Неродные просторы", loading="lazy")
          source(media="(min-width: 1200px)", srcset="~@/img/photo-sunset-desktop@1x.jpg, ~@/img/photo-sunset-desktop@2x.jpg 2x", type="image/jpeg", alt="Неродные просторы", loading="lazy")
          source(media="(min-width: 768px)", srcset="~@/img/photo-sunset-tablet@1x.webp, ~@/img/photo-sunset-tablet@2x.webp 2x", type="image/webp", alt="Неродные просторы", loading="lazy")
